<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Capa para el borde amarillo -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFDD00"/> <!-- Color del borde amarillo -->
        </shape>
    </item>

    <!-- Capa para el fondo blanco dentro del borde -->
    <item android:left="4dp" android:right="4dp" android:top="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/> <!-- Color del fondo blanco -->
        </shape>
    </item>
</layer-list>
